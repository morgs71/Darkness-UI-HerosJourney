<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
	<Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />

<!-- Modified by Sylphan in 2016 -->

<!-- Removed all gaps and borders, and Done buttons stretch to fit any shape (even one-wide) -->
<!-- Only the upper right corner can be used for resizing -->
<!-- On TS containers, the icon serves as the combine button -->
<!-- Done buttons (if any) can be moved to top, middle, or bottom by changing the order of the Pieces lines -->

<!-- Done buttons (if any) are at the bottom in this version -->

	<Ui2DAnimation item = "C_My_Container_ClearBorder">
		<Cycle>false</Cycle>
		<Frames>
			<Texture>window_pieces05.tga</Texture>
			<Location>
				<X>38</X>
				<Y>234</Y>
			</Location>
			<Size>
				<CX>4</CX>
				<CY>4</CY>
			</Size>
		</Frames>
	</Ui2DAnimation>
	<Ui2DAnimation item = "C_My_Container_NoClickBorder">
		<Cycle>false</Cycle>
		<Frames>
			<Texture>window_pieces05.tga</Texture>
			<Location>
				<X>38</X>
				<Y>234</Y>
			</Location>
			<Size>
				<CX>0</CX>
				<CY>4</CY>
			</Size>
		</Frames>
	</Ui2DAnimation>
	<WindowDrawTemplate item="WDT_C_RoundedContainer">
		<Background>wnd_bg_light_rock.tga</Background>
		<VSBTemplate>
			<UpButton>
