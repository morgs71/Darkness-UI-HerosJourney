				<OverlapLeft>0</OverlapLeft>
				<OverlapTop>0</OverlapTop>
				<OverlapRight>0</OverlapRight>
				<OverlapBottom>0</OverlapBottom>
			</Thumb>
			<MiddleTextureInfo>scrollbar_Hgutter.tga</MiddleTextureInfo>
			<MiddleTint>
				<Alpha>255</Alpha>
				<R>128</R>
				<G>128</G>
				<B>128</B>
			</MiddleTint>
